#region-number_guides.container
  .region
    .page-header
      %h1= page_title!('Numero de Guia')

  .region
    %p
      %b Numero:
      = @number_guide.numero

  .region
    = link_to '<span class="glyphicon glyphicon-edit"></span> Editar'.html_safe, edit_number_guide_path(@number_guide), class: 'btn btn-success'
    = link_to '<span class="glyphicon glyphicon-arrow-left"></span> Regresar'.html_safe, number_guides_path, class: 'btn btn-warning'
